<div class="weatherWidget">
    {{#each partials.weathernowContext.locations}}
        <div class="location">
            <h3>{{name}}</h3>
            <a href="{{forecastUrl}}">
                <img src="{{iconUrl}}" alt="{{weather}}">
                {{weathernow}}, {{temp}}
            </a>
        </div>
    {{/each}}
    <small>Источник: <a href="http://www.wunderground.com">Weather
        Underground</a></small>
</div>